#!/home/logix/PycharmProjects/InfoMap/venv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'infomap==1.0.0b15','console_scripts','infomap'
__requires__ = 'infomap==1.0.0b15'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('infomap==1.0.0b15', 'console_scripts', 'infomap')()
    )
